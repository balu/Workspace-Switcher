<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <!-- Unique ID for extension. Can be in e-mail address format or GUID format -->
    <em:id>workspaceswitcher@baluks.gmail.com</em:id>
    <!-- Indicates that this add-on is an extension -->
    <em:type>2</em:type>
    <!-- Extension name displayed in Add-on Manager -->
    <em:name>Workspace Switcher</em:name>
    <!-- Extension version number. There is a version numbering scheme you must follow -->
    <em:version>0.4</em:version>
    <!-- Brief description displayed in Add-on Manager -->
    <em:description>
	Allows you to switch between sets of tabs (like switching between sets of applications in a window manager between multiple desktops. This is useful if you tend to have too many tabs open at a time.
    </em:description>
    <!-- Name of extension's primary developer -->
    <em:creator>Balagopal Komarath</em:creator>
    <!-- Web page address through which extension is distributed -->
    <em:homepageURL>http://github.com/balu/Workspace-Switcher</em:homepageURL>
    <!-- This section gives details of the target application for the extension -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>3.6</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:optionsURL>chrome://workspaceswitcher/content/options.xul</em:optionsURL>

  </Description>
</RDF>
